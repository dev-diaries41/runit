{
  "expo": {
    "name": "RunIt",
    "slug": "runit",
    "version": "1.0.14",
    "orientation": "portrait",
    "icon": "./assets/images/runit-icon.png",
    "scheme": "runit",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "blockedPermissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "minSdkVersion": 29,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/runit_foreground.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.android.runit",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/runit-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "489a36c5-59b5-402c-9638-e80503e82e22"
      }
    }
  }
}
